{
  "version": 2,
  "buildCommand": "cd src/server && npm run build:client",
  "outputDirectory": "src/client/dist",
  "framework": "vite",
  "installCommand": "cd src/server && npm run install:client",
  "devCommand": "cd src/server && npm run dev:client",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
